{
    "name": "lightdash",
    "version": "0.1411.0",
    "main": "index.js",
    "license": "MIT",
    "private": true,
    "resolutions": {
        "@types/react": "^18.2.48",
        "@types/react-dom": "^18.2.18"
    },
    "dependencies": {
        "concurrently": "^8.2.0",
        "npm-run-all2": "^6.0.6",
        "tslib": "^2.6.2"
    },
    "devDependencies": {
        "@types/jest": "^29.5.5",
        "@types/js-yaml": "^4.0.1",
        "@types/node": "^20.8.0",
        "@types/pegjs": "^0.10.3",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "confusing-browser-globals": "^1.0.11",
        "danger": "^11.3.1",
        "dotenv": "^16.0.2",
        "dotenv-cli": "^6.0.0",
        "eslint": "^8.23.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-react": "^1.1.7",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^27.4.2",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-testing-library": "^5.6.3",
        "husky": "^7.0.0",
        "jest": "^29.7.0",
        "lint-staged": "^12.1.2",
        "prettier": "2.8.1",
        "prettier-plugin-organize-imports": "^3.1.1",
        "ts-jest": "^29.1.1",
        "tsx": "4.11.0",
        "typescript": "^4.9.5"
    },
    "scripts": {
        "common-dev": "pnpm --filter @lightdash/common dev",
        "common-build": "pnpm --filter @lightdash/common build",
        "common-lint": "pnpm --filter @lightdash/common lint",
        "common-format": "pnpm --filter @lightdash/common format",
        "common-fix-lint": "pnpm --filter @lightdash/common fix-lint",
        "common-fix-format": "pnpm --filter @lightdash/common fix-format",
        "common-test": "pnpm --filter @lightdash/common test",
        "backend-dev": "pnpm --filter backend dev",
        "backend-build": "pnpm --filter backend build",
        "backend-start": "pnpm --filter backend start",
        "backend-lint": "pnpm --filter backend lint",
        "backend-format": "pnpm --filter backend format",
        "backend-fix-lint": "pnpm --filter backend fix-lint",
        "backend-fix-format": "pnpm --filter backend fix-format",
        "backend-test": "pnpm --filter backend test",
        "frontend-dev": "pnpm --filter frontend start",
        "frontend-build": "pnpm --filter frontend build",
        "frontend-lint": "pnpm --filter frontend lint",
        "frontend-format": "pnpm --filter frontend format",
        "frontend-fix-lint": "pnpm --filter frontend fix-lint",
        "frontend-fix-format": "pnpm --filter frontend fix-format",
        "frontend-test": "pnpm --filter frontend test",
        "e2e-open": "pnpm --filter e2e cypress:open",
        "e2e-run": "pnpm --filter e2e cypress:run",
        "e2e-lint": "pnpm --filter e2e lint",
        "e2e-format": "pnpm --filter e2e format",
        "e2e-fix-lint": "pnpm --filter e2e fix-lint",
        "e2e-fix-format": "pnpm --filter e2e fix-format",
        "cli-test": "pnpm --filter @lightdash/cli test",
        "cli-lint": "pnpm --filter @lightdash/cli lint",
        "cli-format": "pnpm --filter @lightdash/cli format",
        "cli-fix-lint": "pnpm --filter @lightdash/cli fix-lint",
        "cli-fix-format": "pnpm --filter @lightdash/cli fix-format",
        "cli-build": "pnpm --filter @lightdash/cli build",
        "warehouses-dev": "pnpm --filter @lightdash/warehouses dev",
        "warehouses-lint": "pnpm --filter @lightdash/warehouses lint",
        "warehouses-format": "pnpm --filter @lightdash/warehouses format",
        "warehouses-fix-lint": "pnpm --filter @lightdash/warehouses fix-lint",
        "warehouses-fix-format": "pnpm --filter @lightdash/warehouses fix-format",
        "warehouses-test": "pnpm --filter @lightdash/warehouses test",
        "warehouses-build": "pnpm --filter @lightdash/warehouses build",
        "lint": "run-s common-lint backend-lint frontend-lint e2e-lint cli-lint",
        "format": "run-s common-format backend-format frontend-format e2e-format cli-format",
        "fix-lint": "run-s common-fix-lint backend-fix-lint frontend-fix-lint e2e-fix-lint cli-fix-lint",
        "fix-format": "run-s common-fix-format backend-fix-format frontend-fix-format e2e-fix-format cli-fix-format",
        "test": "run-s common-test warehouses-test cli-test backend-test frontend-test",
        "dev": "concurrently -n \"common,warehouses,backend,frontend\" --prefix-colors \"bgWhite.bold,bgGreenBright.bold,bgMagenta.bold,bgBlue.bold\" \"pnpm:common-dev\" \"pnpm:warehouses-dev\" \"pnpm:backend-dev\" \"pnpm:frontend-dev\"",
        "build": "run-s common-build warehouses-build backend-build frontend-build",
        "build-published-packages": "run-s common-build warehouses-build cli-build",
        "start": "run-p backend-start",
        "prepare": "husky install",
        "load:env": "dotenv -c -e .env.development.local",
        "generate-api": "pnpm common-build && pnpm --filter backend generate-api && pnpm --filter backend formatter --write ./src/generated",
        "danger-local-lint": "danger -b main -f",
        "danger-ci-lint": "[[ -n $GITHUB_TOKEN ]] && danger ci --use-github-checks || echo 'Skipped - no GITHUB_TOKEN' present",
        "preinstall": "npx only-allow pnpm",
        "install:common": "pnpm --filter @lightdash/common install",
        "install:frontend": "pnpm --filter frontend install",
        "install:backend": "pnpm --filter backend install",
        "install:warehouses": "pnpm --filter @lightdash/warehouses install",
        "install:cli": "pnpm --filter @lightdash/cli install"
    },
    "lint-staged": {
        "packages/frontend/src/**/*.(ts|tsx|json|css)": [
            "pnpm --filter frontend linter --fix",
            "pnpm --filter frontend formatter --write"
        ],
        "packages/backend/src/**/*.(ts|tsx|json)": [
            "pnpm --filter backend linter --fix",
            "pnpm --filter backend formatter --write"
        ],
        "packages/common/src/**/*.(ts|tsx|json)": [
            "pnpm --filter @lightdash/common linter --fix",
            "pnpm --filter @lightdash/common formatter --write"
        ],
        "packages/e2e/cypress/**/*.(ts|json)": [
            "pnpm --filter e2e linter --fix",
            "pnpm --filter e2e formatter --write"
        ],
        "packages/cli/src/**/*.(ts|tsx|json)": [
            "pnpm --filter @lightdash/cli linter --fix",
            "pnpm --filter @lightdash/cli formatter --write"
        ],
        "packages/warehouses/src/**/*.(ts|tsx|json)": [
            "pnpm --filter @lightdash/warehouses linter --fix",
            "pnpm --filter @lightdash/warehouses formatter --write"
        ]
    }
}
